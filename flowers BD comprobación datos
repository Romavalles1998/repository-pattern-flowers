INSERT INTO flor (name) VALUES ('Girasol');
INSERT INTO flor (name) VALUES ('Orquídea');
INSERT INTO flor (name) VALUES ('Clavel');
INSERT INTO flor (name) VALUES ('Dalia');

PARA COMPROBAR DATOS:

SELECT * FROM flor;
SELECT name FROM flor;
SELECT * FROM flor WHERE id = 1;
SELECT COUNT(*) FROM flor;

